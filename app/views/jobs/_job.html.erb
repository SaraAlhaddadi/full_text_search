<div id="<%= dom_id job %>">
  <p>
    <strong>Country:</strong>
    <%= job.country %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= job.title %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= job.gender %>
  </p>

  <p>
    <strong>Skill:</strong>
    <%= job.skill %>
  </p>

  <p>
    <strong>Sector:</strong>
    <%= job.sector %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= job.company %>
  </p>

  <p>
    <strong>Salary:</strong>
    <%= job.salary %>
  </p>

  <p>
    <strong>Posted at:</strong>
    <%= job.posted_at %>
